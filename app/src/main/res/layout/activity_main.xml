<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <android.widget.GridLayout
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:columnCount="3"
        android:rowCount="3"
        tools:ignore="MissingConstraints">
        <ImageView
            android:id="@+id/img1"
            android:layout_height="360px"
            android:layout_width="360px"
            android:src="@drawable/leona"/>
        <ImageView
            android:id="@+id/img2"
            android:layout_height="360px"
            android:layout_width="360px"
            android:src="@drawable/lissandraclassic"/>
        <ImageView
            android:id="@+id/img3"
            android:layout_height="360px"
            android:layout_width="360px"
            android:src="@drawable/twistedfateclassic"/>
        <ImageView
            android:id="@+id/img4"
            android:layout_height="360px"
            android:layout_width="360px"
            android:src="@drawable/zedclassic"/>
        <ImageView
            android:id="@+id/img5"
            android:layout_height="360px"
            android:layout_width="360px"
            android:src="@drawable/swainclassic"/>
        <ImageView
            android:id="@+id/img6"
            android:layout_height="360px"
            android:layout_width="360px"
            android:src="@drawable/jannaclassic"/>
        <ImageView
            android:id="@+id/img7"
            android:layout_height="360px"
            android:layout_width="360px"
            android:src="@drawable/talon"/>
        <ImageView
            android:id="@+id/img8"
            android:layout_height="360px"
            android:layout_width="360px"
            android:src="@drawable/kaisaclassic"/>
        <ImageView
            android:id="@+id/img9"
            android:layout_height="360px"
            android:layout_width="360px"
            android:src="@drawable/missfortunegungoddess"/>

    </android.widget.GridLayout>

</LinearLayout
   >